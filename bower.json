{
	"name": "react-ive-meteor",
	"version": "0.0.0",
	"homepage": "https://github.com/rozzzly/react-ive-meteor",
	"authors": [
		"rozzzly <pclienau@gmail.com>"
	],
	"directory": "public/",
	"description": "bower file for the react-ive-meteor repository. This is linked to my fork but be sure to checkout the main repo as well https://github.com/AdamBrodzinski/react-ive-meteor",
	"private": true,
	"keywords": [
		"react",
		"meteor",
		"real-time",
		"chat",
		"social",
		"ddp",
		"gui",
		"ui"
	],
	"license": "MIT",
	"ignore": [
		"**/.*",
		"node_modules",
		"bower_components",
		"test",
		"tests"
	],
	"overrides": {
		"ionsound": {
			"main": [
				"js/ion.sound.js",
                "sounds/water_droplet.aac",
                "sounds/water_droplet.mp3",
                "sounds/water_droplet.ogg",
                "sounds/water_droplet_3.aac",
                "sounds/water_droplet_3.mp3",
                "sounds/water_droplet_3.ogg"
            ]
		}
	},
	"dependencies": {
		"ionsound": "~3.0.4"
	}
}
